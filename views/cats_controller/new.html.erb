<form action="/cats" method="POST">
  <input
    type='hidden'
    name='authenticity_token'
    value='<%= session["authenticity_token"] %>'>
  
  <label>Name
    <input type="text" name="cat[name]" value="<%= @cat.name %>">
  </label>

  <label>Owner
    <input type="text" name="cat[owner]" value="<%= @cat.owner %>">
  </label>

  <input type="submit">
</form>
